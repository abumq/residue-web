{
  "name": "residue-web-ui",
  "version": "0.0.1",
  "description": "Residue web interface for client management",
  "repository": {
    "type": "git",
    "url": "https://github.com/muflihun/residue-web-ui"
  },
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.1",
    "residue": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.0.2",
    "mz": "^2.4.0",
    "react-scripts": "0.2.1",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "scripts": {
    "start": "nodemon server/ --exec babel-node --presets es2015",
    "start:inspect": "nodemon --inspect server/ --exec babel-node --presets es2015",
    "start:old": "react-scripts start --inspect",
    "start:server": "node server",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src/components",
    "test": "mocha test"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js",
    "env": {
      "mocha": true
    },
    "rules": {
      "strict": 0
    }
  }
}
